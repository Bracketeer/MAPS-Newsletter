/* Basics */
$purple1 = #260C40;
$purple2 = #561548;
$purple3 = #623A82;
$purple4 = #976BAE;
$button = #FFA50A;
$button-hover = #DC7B0D;
$light-text = #777777;
$hr-color = #CCCCCC;
$light = #EEEEEE;

.padded{
  padding: 10px;
}
body {
  Margin: 0;
  padding: 0;
  min-width: 100%;
  background-color: $light;
}
table.background{
  background: $purple1;
}
table.background-two{
  background: $purple2;
}
table.background-three{
  background: $purple3;

}
table.background-four{
  background: $purple4;

}
.light{
  color: $light !important;
}
table {
  border-spacing: 0;
  font-family: sans-serif;
  color: #333333;
}
td {
  padding: 0;
}
img {
  border: 0;
}
.wrapper {
  width: 100%;
  table-layout: fixed;
  -webkit-text-size-adjust: 100%;
  -ms-text-size-adjust: 100%;
}
.webkit {
  max-width: 600px;
}
.outer {
  Margin: 0 auto;
  width: 100%;
  max-width: 600px;
}
.full-width-image img {
  width: 100%;
  max-width: 600px;
  height: auto;
}
.full-width-image p {
  font-size: 14px;
  padding: 0px;
}
.inner {
  padding: 10px;
}
p {
  Margin: 0;
  font-size: 15px;
  padding: 5px;
}
.button a {
  color: #ffffff;
  text-decoration: none;
}
ol, ul{
  font-size: 15px;
}
a{
  color: $button;
  text-decoration: none;
}
.h1 {
  font-size: 28px !important;
  font-weight: 100;
  color: $light;
  text-align: center;
  padding: 10px;

}
.h2 {
  font-size: 20px !important;
  font-weight: normal;
  Margin-bottom: 12px;
  padding-left: 10px;
  padding-right: 10px;
  color: #124062;
}
.h3{
  font-size: 16px !important;
  Margin-bottom: 12px;
  font-weight: bold;
}
/* One column layout */
.one-column .contents {
  text-align: left;
}
.one-column p {
  font-size: 14px;
  Margin-bottom: 10px;
}
/*Two column layout*/
.two-column {
  text-align: center;
  font-size: 0;
}
.two-column .column {
  width: 100%;
  max-width: 300px;
  display: inline-block;
  vertical-align: middle;
}
.four-column .contents {
  text-align: center;
  font-size: 12px;
  color: $light;
  height: 300px;
  vertical-align: top;
}
.four-column .column {
  width: 100%;
  max-width: 140px;
  display: inline-block;
  vertical-align: top;
}
.contents {
  width: 100%;
}
.two-column .contents {
  font-size: 14px;
  text-align: center;
  color: #eee;
  height: 200px;

}
.two-column .contentshead {
  font-size: 14px;
  text-align: center;
  color: #eee;
}
.two-column {
  width: 100%;
  max-width: 260px;
  height: auto;
}
.two-column .text {
  padding-top: 10px;
}
/*Three column layout*/
.three-column {
  text-align: center;
  font-size: 0;
  padding-top: 10px;
  padding-bottom: 10px;
}
.three-column .column {
  width: 100%;
  max-width: 200px;
  display: inline-block;
  vertical-align: top;
}
.three-column .contents {
  font-size: 14px;
  text-align: center;
}
.three-column img {
  width: 100%;
  max-width: 180px;
  height: auto;
}
.three-column .text {
  padding-top: 10px;
}
.header{
  background: $purple1;
  color: #ffffff;
  // Margin-bottom: 30px;
}
.headerCTA{
  padding-bottom: 10px
}
.footer{
  background: #124062;
  color: #ffffff;
}
table.button td{
  // background: $purple1;
  padding: 10px 30px;
  color: #ffffff;
  Margin: 10px;
  // width: 100%;
  text-align: center;
  border-radius: 4px;
  border: 1px solid $light;
}
// table.button td:hover{
//   background: $purple3 !important;
// }

table.secondary td {
  background: $button !important;
  border-color: $button;
  border-radius: 0;
  color: $light;
}

table.secondary td a {
  color: $light;
  background: $button !important;
}

table.secondary:hover td {
  background: $button-hover !important;
  color: #fff;
}

table.secondary:hover td a,
table.secondary td a:visited,
table.secondary:active td a {
  color: $light !important;
}

hr {
color: #cccccc;
background-color: #d9d9d9;
height: 1px;
border: none;
Margin-top: 10px;
Margin-bottom: 10px;
}
/* Left sidebar layout */
.left-sidebar {
  text-align: left;
    font-size: 0;
}
.left-sidebar .column {
    width: 100%;
    display: inline-block;
    vertical-align: middle;
}
.left-sidebar .left {
    max-width: 100px;
}
.left-sidebar .right {
    max-width: 500px;
}
.left-sidebar .img {
    width: 100%;
    max-width: 500px;
    height: auto;
}
.cta{
  padding 10px;
  color: $light;

}
.left-sidebar .contents {
    font-size: 14px;
    text-align: left;
    padding: 10px;
}
/* Right sidebar layout */
.right-sidebar {
  text-align: center;
  font-size: 0;
}
.right-sidebar .column {
    width: 100%;
    display: inline-block;
    vertical-align: middle;
}
.right-sidebar .left {
    max-width: 80px;
}
.right-sidebar .right {
    max-width: 500px;
}
.right-sidebar .img {
    width: 100%;
    max-width: 80px;
    height: auto;
}
.right-sidebar .contents {
    font-size: 14px;
    text-align: left;
    padding: 10px;
}
.panel {
  background: #f2f2f2;
  border: 1px solid #d9d9d9;
  padding: 10px !important;
  Margin-top: 10px;
  Margin-left: 10px;
  Margin-right: 10px;
}
.hangout {
  background: #f2f2f2;
  border: 1px solid #d9d9d9;
  Margin-top: 60px;
  padding-top: 40px;
  Margin-bottom: 60px;
  padding-bottom: 70px;
}
.spacer{
  padding: 30px;
}
.urgent{
  color: red;
  font-size: 16px !important;
}
.footer{
  color: #d8d8d8;
  font-size: 12px !important;
  padding: 10px;
}
.text-left{
  padding: 10px;
  background: $light;
}
.text-right{
  padding: 10px;
  background: #dbdbdb;
}
/*Media Queries*/
@media screen and (max-width: 400px) {
  .two-column .column,
  .three-column .column {
      max-width: 100% !important;
  }
  .two-column img {
      max-width: 100% !important;
  }
  .three-column img {
      max-width: 50% !important;
  }
}
@media screen and (min-width: 401px) and (max-width: 620px) {
  table.button td{
    padding: 10px 10px !important;
  }
  .three-column .column {
      max-width: 33% !important;
  }
  .two-column .column {
      max-width: 50% !important;
  }
}
@media only screen and (max-width: 599px) {
      div[class="pattern"] video,
      div[class="pattern"] img
      {
          width: 100%;
          height: auto !important;
      }
  }
